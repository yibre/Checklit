{
  // Required
  "manifest_version": 2,
  "name": "Starry Diary",
  "version": "1.0",

  // Recommended
  "description": "Quick launch Traversy Media",
  "icons": {"128": "icon128.png"},

  // Pick one (or none)
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "calendar.html"
  },

  "permissions": ["tabs"]
}